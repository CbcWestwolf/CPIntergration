<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>

    <ui:style>

        @eval outputBackgroundColor org.eclipse.che.ide.api.theme.Style.theme.outputBackgroundColor();

        .resultPanel{
            width: 100%;
            height: 100%;
        }

        .countPanel {
            width: 100%;
            overflow: hidden;
            background-color: #333;
        }

        .contentPanel{
            width: 100%;
            height: 96%;
            overflow: scroll;
        }

        .apiPanel{
            width: 100%;
        }

        .mainPanel{
            width: 100%;
        }

        .posPanel{
            width: 100%;
            /*height: 100%;*/
        }


    </ui:style>



    <g:FlowPanel addStyleNames="{style.resultPanel}">
        <g:FlowPanel ui:field="countPanel" styleName="{style.countPanel}">

        </g:FlowPanel>
        <g:FlowPanel addStyleNames="{style.contentPanel}">
            <g:FlowPanel ui:field="apiPanel" styleName="{style.apiPanel}">

            </g:FlowPanel>
            <g:FlowPanel ui:field="mainPanel" addStyleNames="{style.mainPanel}">

            </g:FlowPanel>
            <g:FlowPanel ui:field="posPanel" addStyleNames="{style.posPanel}">

            </g:FlowPanel>
        </g:FlowPanel>

    </g:FlowPanel>




</ui:UiBinder>