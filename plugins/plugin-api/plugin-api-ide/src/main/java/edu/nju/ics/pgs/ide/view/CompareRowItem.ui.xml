            <!--</g:FlowPanel>-->
        </g:FlowPanel>
        <g:FlowPanel ui:field="compareRowPanel">
            <!--<g:FlowPanel>-->

            <!--</g:FlowPanel>-->
        </g:FlowPanel>
    </g:FlowPanel>




</ui:UiBinder>