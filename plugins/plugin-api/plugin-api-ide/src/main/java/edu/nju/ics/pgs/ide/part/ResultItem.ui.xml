<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>

    <ui:style>

        @eval outputBackgroundColor org.eclipse.che.ide.api.theme.Style.theme.outputBackgroundColor();

        .resultPanel {
            width: 94%;
            padding: 1%;
            margin-left: 2%;
            margin-right: 2%;
            margin-top: 2px;
            margin-bottom: 2px;
        }

        .resultPanel:hover{
            background-color: #444;
            color: #EEE;
        }




    </ui:style>


        <g:FocusPanel ui:field="resultItemPanel" styleName="{style.resultPanel}">

        </g:FocusPanel>



</ui:UiBinder>